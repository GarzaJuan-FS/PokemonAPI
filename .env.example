# Example environment variables for Pokemon API
# Copy this file to .env and update with your actual values

# MongoDB connection string
# For local development: mongodb://localhost:27017/pokemon
# For MongoDB Atlas: mongodb+srv://username:password@cluster.mongodb.net/pokemon
DATABASE_URL=mongodb://localhost:27017/pokemon

# Server port (Heroku and other platforms will override this)
PORT=4000

# React App API URL for production deployment
# Replace 'your-app-name' with your actual Heroku app name or domain
REACT_APP_API_URL=https://your-app-name.herokuapp.com
